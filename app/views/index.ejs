<script>
  $(document).ready(function(){
    astroUp();
    <% if(errMessage && errMessage.length>0) { %>
        Materialize.toast(<%- JSON.stringify(errMessage) %>, 10000)
    <% } %>

    <% if(message && message.length>0) { %>
        Materialize.toast(<%- JSON.stringify(message) %>, 10000)
    <% } %>
  });

  function astroDown(){
    $('.astronaut').removeClass('translate-down-astro');
    $('.astronaut').addClass('translate-up-astro');
    setTimeout(astroUp,4000);
  }
  function astroUp(){
    $('.astronaut').removeClass('translate-up-astro');
    $('.astronaut').addClass('translate-down-astro');
    setTimeout(astroDown,3000);
  }
</script>
